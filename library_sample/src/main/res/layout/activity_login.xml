    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ui.LoginActivity">
    <data>
        <variable
            name="vm"
            type="com.example.library_user.model.SecondModel" />
    </data>
    <FrameLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent">
